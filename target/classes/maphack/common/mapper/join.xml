<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="join">

	<!-- 회원가입 -->
	<insert id="makeMember" parameterType="maphack.join.dto.Member">
		insert into member values (#{mId},#{mPass},#{mNickname},#{mName},#{mMobile},#{mEmail},#{mGender},'1992/01/01','imsi.jpg')
	</insert>
</mapper>












